<div class="row">
  <div class="col-md-3">
    <ul class="sidebar-list">
      <li class="sidebar-item"> <%= link_to "Your Listings", parkings_path, class: "sidebar-link active" %> </li>
      <li class="sidebar-item"> <%= link_to "Your Reservations", your_reservations_path, class: "sidebar-link " %> </li>
      <li class="sidebar-item"> <%= link_to "Your Spots", your_spots_path, class: "sidebar-link" %> </li>

    </ul>
  </div>
  <div class="col-md-9">
    <div class="panel panel-default">
      <div class="panel-heading">
        Listings
      </div>
      <div class="panel-body">

        <% @parkings.each do |parking| %>
          <div class="row">
            <div class="col-md-2">
              <%= image_tag parking.cover_photo(:thumb) %>
            </div>
            <div class="col-md-7">
              <h4><%= parking.listing_name %></h4>
            </div>
            <div class="col-md-3 right">
                <%= link_to "Update", listing_parking_path(parking), class: "btn btn-form" %>
            </div>
          </div>
          <hr>
        <% end %>
      </div>
    </div>
  </div>
</div>
